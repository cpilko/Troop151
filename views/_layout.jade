doctype html
html
  block vars
    -var pageTitle = 'Layout'
    -var pageDesc = 'Description of this page'

  head
    meta(charset='utf8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge, chrome=1')
    meta(name='description', content="#{pageDesc}")
    meta(name='author', content= "Troop 151 Broomall, PA")
    meta(name="viewport" content="width=device-width, initial-scale=1")
    title= pageTitle
    != css()

  body
    include _header.jade
    block content
    include _foot.jade

    != js()
